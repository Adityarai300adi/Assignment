#include<stdio.h>
int main(){
	int l,b,h,tsa,volume;
	printf("Enter the length: ");
	scanf("%d",&l);
	printf("Enter the breath: ");
	scanf("%d",&b);
	printf("Enter the height: ");
	scanf("%d",&h);
	tsa=2*(l*b+b*h+l*h);
	printf("The total surface area of cubiod is %d",tsa);
	printf(" unit square");
	volume=l*b*h;
	printf("\nThe volume of cuboid is %d",volume);
	printf(" unit cube");
	return 0;
}
