#include <stdio.h>
#include <math.h>
 int main(){
    float sideOne, sideTwo, sideThree, s, area;
     
    printf("Enter the length of sideOne of triangle\n");
    scanf("%f", &sideOne);
    printf("Enter the length of sideTwo of triangle\n");
    scanf("%f", &sideTwo);
    printf("Enter the length of sideThree of triangle\n");
    scanf("%f", &sideThree);
    s = (sideOne + sideTwo + sideThree)/2; //Semiperimeter 
    area = sqrt(s*(s-sideOne)*(s-sideTwo)*(s-sideThree));
    printf("Area of triangle : %0.4f", area);
    printf(" unit square");
     
    return 0;
}
